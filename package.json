{
  "name": "echo-editor-in",
  "version": "0.4.1",
  "private": false,
  "license": "MIT",
  "description": "A modern WYSIWYG rich-text editor using tiptap for Vue.js",
  "author": "",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "dev": "vite",
    "examples": "pnpm --dir ./examples run dev",
    "build:lib": "vite build",
    "build:examples": "pnpm build:lib &&pnpm --dir ./examples run build",
    "preview": "vite preview",
    "git-commit": "git-cz",
    "prettier": "prettier --write .",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.vue src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx,.vue src --fix",
    "push": "pnpm build:lib && pnpm publish --no-git-checks --registry https://registry.npmjs.org/",
    "prepare": "husky"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "keywords": [
    "echo editor",
    "WYSIWYG",
    "vue",
    "vue.js",
    "typescript",
    "tiptap",
    "editor",
    "shadcn ui",
    "wysiwyg",
    "rich text",
    "markdown"
  ],
  "main": "./lib/echo-editor.umd.js",
  "module": "./lib/echo-editor.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/echo-editor.mjs",
      "require": "./lib/echo-editor.umd.js"
    },
    "./style.css": {
      "import": "./lib/style.css",
      "require": "./lib/style.css"
    }
  },
  "sideEffects": false,
  "files": [
    "lib",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": "^18.0.0"
  },
  "dependencies": {
    "@radix-icons/vue": "^1.0.0",
    "@tiptap/core": "^2.11.5",
    "@tiptap/extension-blockquote": "^2.11.5",
    "@tiptap/extension-bold": "^2.11.5",
    "@tiptap/extension-bubble-menu": "^2.11.5",
    "@tiptap/extension-bullet-list": "^2.11.5",
    "@tiptap/extension-character-count": "^2.11.5",
    "@tiptap/extension-code": "^2.11.5",
    "@tiptap/extension-code-block": "^2.11.5",
    "@tiptap/extension-code-block-lowlight": "^2.11.5",
    "@tiptap/extension-color": "^2.11.5",
    "@tiptap/extension-document": "^2.11.5",
    "@tiptap/extension-dropcursor": "^2.11.5",
    "@tiptap/extension-focus": "^2.11.5",
    "@tiptap/extension-font-family": "^2.11.5",
    "@tiptap/extension-gapcursor": "^2.11.5",
    "@tiptap/extension-hard-break": "^2.11.5",
    "@tiptap/extension-heading": "^2.11.5",
    "@tiptap/extension-highlight": "^2.11.5",
    "@tiptap/extension-history": "^2.11.5",
    "@tiptap/extension-horizontal-rule": "^2.11.5",
    "@tiptap/extension-image": "^2.11.5",
    "@tiptap/extension-italic": "^2.11.5",
    "@tiptap/extension-link": "^2.11.5",
    "@tiptap/extension-list-item": "^2.11.5",
    "@tiptap/extension-ordered-list": "^2.11.5",
    "@tiptap/extension-paragraph": "^2.11.5",
    "@tiptap/extension-placeholder": "^2.11.5",
    "@tiptap/extension-strike": "^2.11.5",
    "@tiptap/extension-subscript": "^2.11.5",
    "@tiptap/extension-superscript": "^2.11.5",
    "@tiptap/extension-table": "^2.11.5",
    "@tiptap/extension-table-cell": "^2.11.5",
    "@tiptap/extension-table-header": "^2.11.5",
    "@tiptap/extension-table-row": "^2.11.5",
    "@tiptap/extension-task-item": "^2.11.5",
    "@tiptap/extension-task-list": "^2.11.5",
    "@tiptap/extension-text": "^2.11.5",
    "@tiptap/extension-text-align": "^2.11.5",
    "@tiptap/extension-text-style": "^2.11.5",
    "@tiptap/extension-underline": "^2.11.5",
    "@tiptap/pm": "^2.11.5",
    "@tiptap/suggestion": "^2.11.5",
    "@tiptap/vue-3": "^2.11.5",
    "@vueuse/core": "^10.9.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "echo-drag-handle-plugin": "^0.0.2",
    "hotkeys-js": "^3.13.7",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@iconify/vue": "^4.1.2",
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.0",
    "commitlint": "^19.3.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.26.0",
    "express": "^4.19.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "lodash-unified": "^1.0.3",
    "prettier": "^3.2.5",
    "radix-vue": "^1.7.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.75.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.0",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.6"
  },
  "peerDependencies": {
    "vue": ">= 3.0.0"
  }
}
